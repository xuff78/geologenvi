<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:gravity="center"
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:id="@+id/txtNum"
                style="@style/txt_gray_11" />

            <TextView
                android:gravity="center"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:id="@+id/txt1"
                style="@style/txt_gray_11" />

            <TextView
                android:gravity="center"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:id="@+id/txt2"
                style="@style/txt_gray_11" />

            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="240dp"
                android:layout_height="50dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/txt3"
                        style="@style/txt_gray_10" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt4"
                        style="@style/txt_gray_10" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt5"
                        style="@style/txt_gray_10" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/txt6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        style="@style/txt_gray_10" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="480dp"
                android:layout_height="50dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt7"
                        style="@style/txt_gray_11" />


                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_width="160dp"
                        android:layout_height="50dp">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="50dp">
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt8"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt9"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"

                                android:id="@+id/txt10"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt11"
                                style="@style/txt_gray_11" />
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt12"
                                style="@style/txt_gray_11" />


                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_width="160dp"
                        android:layout_height="50dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="40dp">
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt13"
                                style="@style/txt_gray_11" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="480dp"
                android:layout_height="50dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">



                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_width="160dp"
                        android:layout_height="50dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="50dp">
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt14"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt15"
                                style="@style/txt_gray_11" />



                        </LinearLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_width="160dp"
                        android:layout_height="50dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="50dp">
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/txt16"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"

                                android:id="@+id/txt17"
                                style="@style/txt_gray_11" />



                        </LinearLayout>

                    </LinearLayout>



                    <LinearLayout
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_width="160dp"
                        android:layout_height="50dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="50dp">
                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"

                                android:id="@+id/txt18"
                                style="@style/txt_gray_11" />

                            <TextView
                                android:layout_weight="1"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"

                                android:id="@+id/txt19"
                                style="@style/txt_gray_11" />



                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="480dp"
                android:layout_height="50dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt20"
                        style="@style/txt_gray_11" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/txt21"
                        style="@style/txt_gray_11" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt22"
                        style="@style/txt_gray_11" />


                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt23"
                        style="@style/txt_gray_11" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt24"
                        style="@style/txt_gray_11" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt25"
                        style="@style/txt_gray_11" />

                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/txt26"
                        style="@style/txt_gray_11" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:gravity="center"
                android:layout_width="70dp"
                android:layout_height="50dp"

                android:id="@+id/txt27"
                style="@style/txt_gray_11" />

            <TextView
                android:gravity="center"
                android:layout_width="70dp"
                android:layout_height="50dp"

                android:id="@+id/txt28"
                style="@style/txt_gray_11" />
        </LinearLayout>

    </LinearLayout>
    <View
        android:background="@color/light_gray"
        android:layout_width="match_parent"
        android:layout_height="1px" />
</LinearLayout>